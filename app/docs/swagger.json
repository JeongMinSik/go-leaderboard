        "/users/:start/to/:stop": {
            "get": {
                "description": "user list 를 받아옵니다.",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Users"
                ],
                "summary": "Get user list",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "start index",
                        "name": "start",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "integer",
                        "description": "stop index",
                        "name": "stop",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/leaderboard.UserRank"
